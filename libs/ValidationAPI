INSTALAR

mvn install:install-file -Dfile=path/to/ValidationAPI.jar -DgroupId=org.netbeans -DartifactId=ValidationAPI -Dversion=1.0 -Dpackaging=jar

mvn install:install-file -Dfile=path/to/org-openide-util.jar -DgroupId=org.netbeans -DartifactId=org-openide-util -Dversion=1.0 -Dpackaging=jar

mvn install:install-file -Dfile=path/to/org-openide-util.ui.jar -DgroupId=org.netbeans -DartifactId=org-openide-util-ui -Dversion=1.0 -Dpackaging=jar

mvn install:install-file -Dfile=path/to/org-openide-modules.jar -DgroupId=org.netbeans -DartifactId=org-openide-modules -Dversion=1.0 -Dpackaging=jar

mvn install:install-file -Dfile=path/to/org-openide-util-lookup.jar -DgroupId=org.netbeans -DartifactId=org-openide-util-lookup -Dversion=1.0 -Dpackaging=jar


CARGAR EN POM (MAVEN)

<dependencies>
    <!-- Validation API -->
    <dependency>
        <groupId>org.netbeans</groupId>
        <artifactId>ValidationAPI</artifactId>
        <version>1.0</version>
    </dependency>

    <!-- OpenIDE Util -->
    <dependency>
        <groupId>org.netbeans</groupId>
        <artifactId>org-openide-util</artifactId>
        <version>1.0</version>
    </dependency>

    <!-- OpenIDE Util UI -->
    <dependency>
        <groupId>org.netbeans</groupId>
        <artifactId>org-openide-util-ui</artifactId>
        <version>1.0</version>
    </dependency>

    <!-- OpenIDE Modules -->
    <dependency>
        <groupId>org.netbeans</groupId>
        <artifactId>org-openide-modules</artifactId>
        <version>1.0</version>
    </dependency>

    <!-- OpenIDE Util Lookup -->
    <dependency>
        <groupId>org.netbeans</groupId>
        <artifactId>org-openide-util-lookup</artifactId>
        <version>1.0</version>
    </dependency>
</dependencies>
